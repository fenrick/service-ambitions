{
    "model": "openai:gpt-5",
    "models": {
        "descriptions": "openai:o4-mini",
        "features": "openai:gpt-5",
        "mapping": "openai:o4-mini",
        "search": "openai:gpt-4o-search-preview"
    },
    "reasoning": {"effort": "medium"},
    "web_search": false,
    "log_level": "INFO",
    "mapping_types": {
